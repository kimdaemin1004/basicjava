
public class Ex02 {
	public static void main(String[] args) {
		
		// 1부터 10까지 출력하는 for문
		for(int i = 1; i < 11; i ++) {	
			System.out.printf("%d ", i);
			// System.out.print(i + " ");

		}
		System.out.println("\n======================================");
		
		
		// 1부터 10까지 출력하는 while문
		
		/*
		초기화식
		while(조건식) {
          수행문;
          증감식;	
		}
		*/
		
		ㅑㅜㅅ
	}

}
